<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="Ouchihuahua">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.googlecode.mgwt.MGWT"/>
    <inherits name="com.google.gwt.activity.Activity"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.biomined.ouchihuahua.client.OuchihuahuaEntryPoint'/>

    <source path="client"/>
    <source path="shared"/>

    <!-- Specify the app servlets.                   -->
    <servlet path="/UserService" class="com.biomined.ouchihuahua.server.servlets.UserServiceServlet"/>
    <servlet path="/EventService" class="com.biomined.ouchihuahua.server.servlets.EventServiceServlet"/>

</module>